# Telegram Bot Configuration
# 你的 Telegram Bot Token (必填)
TG_BOT_TOKEN=your_bot_token_here
# 管理员的用户 ID (必填，用于权限控制)
ADMIN_ID=123456789

# Telegram API Configuration (Optional)
# 自建反代地址，如果不需要留空
# TG_BASE_URL=https://api.telegram.org
# HTTP 代理地址，如果不需要留空
# TG_PROXY=http://127.0.0.1:7890

# AI Configuration (Gemini)
# Gemini API Key，支持多个 Key 轮询，用逗号分隔 (必填)
GEMINI_API_KEY=key1,key2,key3

# OpenWrt SSH Configuration
# OpenWrt 路由器地址 (必填)
OPENWRT_HOST=192.168.1.1
# SSH 端口 (默认 22)
OPENWRT_PORT=22
# SSH 用户名 (默认 root)
OPENWRT_USER=root
# SSH 密码 (如果使用密钥登录可留空)
OPENWRT_PASS=your_ssh_password
# SSH 密钥路径，可选，请确保将私钥文件重命名为 id_rsa 并放入 bot 目录下的 data 文件夹中
OPENWRT_KEY_FILE=/app/data/id_rsa

# OpenClash Configuration
# OpenClash 面板地址 (默认 http://127.0.0.1:9090)
# 如果 Bot 运行在路由器上，可以使用 localhost
# 如果 Bot 运行在其他设备，需要填写路由器的 IP 和端口
OPENCLASH_API_URL=http://192.168.1.1:9090
# OpenClash API Secret (如果在配置中设置了密钥则必填)
OPENCLASH_API_SECRET=your_secret

# AdGuard Home Configuration
# AdGuard Home 管理面板地址 (必填)
ADG_URL=http://192.168.1.1:3000
# AdGuard Home 用户名 (必填)
ADG_USER=admin
# AdGuard Home 密码 (必填)
ADG_PASS=your_adg_password
